{
  "$schema": "../gen/schemas/desktop-schema.json",
  "description": "Capability for the main window",
  "identifier": "default",
  "permissions": [
    "core:default",
    "opener:allow-open-url",
    "opener:allow-default-urls",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-mkdir",
    "dialog:default"
  ],
  "remote": {
    "urls": ["https://*"]
  },
  "windows": ["main"]
}
